{
  "name": "@expo/auth-redirect",
  "private": true,
  "version": "1.0.0",
  "description": "A Cloud Function to redirect authentication requests to third-party authentication services",
  "author": "Expo",
  "license": "UNLICENSED",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "rm -rf build",
    "deploy-production": "scripts/deploy.sh -e production",
    "deploy-staging": "scripts/deploy.sh -e staging",
    "dev": "scripts/develop.sh",
    "lint": "eslint src",
    "start": "functions-framework --target default --port 3031",
    "test": "jest",
    "test-e2e": "jest --config jest-e2e.config.json",
    "test-integration": "yarn build && yarn jest --config jest-integration.config.json"
  },
  "volta": {
    "node": "16.8.0"
  },
  "dependencies": {
    "express": "^4.17.3",
    "serve-handler": "^6.1.3"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^1.9.0",
    "@tsconfig/node14": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/node": "^16.7.5",
    "@types/node-fetch": "^2.5.12",
    "@types/serve-handler": "^6.1.1",
    "eslint": "^7.32.0",
    "eslint-config-universe": "^7.0.1",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "typescript": "^4.6.3"
  }
}
